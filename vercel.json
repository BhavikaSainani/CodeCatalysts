{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin-allow-popups"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "unsafe-none"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://0.0.0.0:8001/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}